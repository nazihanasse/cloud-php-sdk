  ],
  "require": {
    "php" : ">=7",
    "guzzlehttp/psr7": "1.4.*",
    "guzzlehttp/guzzle": "6.2.*",
    "doctrine/annotations": "1.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.1.*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "SkyCentrics\\Cloud\\" : "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
      "SkyCentrics\\Cloud\\Tests\\" : "tests/"
    }
  }
}